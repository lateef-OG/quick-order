{
  "expo": {
    "name": "quick_order_mobile",
    "slug": "quick_order_mobile",
    "icon": "./src/assets/img/food.png",
    "splash": {
      "image": "./src/assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Inter-Medium.ttf",
            "./src/assets/fonts/Inter-Regular.ttf",
            "./src/assets/fonts/Inter-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Quick Order to access your camera",
          "microphonePermission": "Allow Quick Order to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}